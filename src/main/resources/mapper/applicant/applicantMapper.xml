<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.piveguyz.empickbackend.employment.applicant.query.mapper.ApplicantMapper">

    <select id="findAllApplicant" resultType="com.piveguyz.empickbackend.employment.applicant.query.dto.ApplicantQueryDTO">
        SELECT
            id,
            name,
            phone,
            email,
            profile_url,
            birth,
            address
        FROM applicant
    </select>

</mapper>
